{
  "name": "singelong333",
  "displayName": "SingeLong333",
  "description": "VSCode extension to singelong (read: sing along) played music connected with your spotify",
  "version": "1.2.0",
  "engines": {
    "vscode": "^1.78.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benjas333/singelong333"
  },
  "icon": "assets/images/icon.png",
  "__metadata": {
    "publisherDisplayName": "Benjas333"
  },
  "publisher": "Benjas333",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "singelong333-activitybar",
          "title": "SingeLong333",
          "icon": "assets/images/icon-logo.svg"
        }
      ]
    },
    "views": {
      "singelong333-activitybar": [
        {
          "icon": "assets/images/icon-logo.svg",
          "type": "webview",
          "id": "singelong333.openview",
          "name": "SingeLong333"
        }
      ]
    },
    "commands": [
      {
        "command": "singelong333.authorize",
        "title": "Authorize",
        "category": "SingeLong333"
      },
      {
        "command": "singelong333.logout",
        "title": "Logout",
        "category": "SingeLong333"
      },
      {
        "command": "singelong333.reload-panel",
        "title": "Reload Panel (Debugging)",
        "category": "SingeLong333"
      }
    ],
    "menus": {
      "webview/context": [
        {
          "command": "singelong333.reload-panel",
          "when": "webviewId == 'singelong333.openview'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "webpack_compile": "webpack --mode development",
    "webpack_watch": "webpack --mode development --watch",
    "webpack_vscode:prepublish": "npm run package",
    "webpack_package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.78.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.9.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.5.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.7.7",
    "express": "^4.19.2",
    "kuroshiro": "^1.2.0",
    "kuroshiro-analyzer-kuromoji": "^1.1.0"
  }
}
